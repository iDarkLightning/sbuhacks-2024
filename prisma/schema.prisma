// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Book {
  id       String   @id @default(cuid())
  isbn     String   @unique
  title    String
  author   String[]
  subjects String[]
  ownedBy  String[]
}
